<?xml version="1.0"?>
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
  <specVersion>
    <major>1</major>
    <minor>0</minor>
  </specVersion>
  <serviceStateTable>
	<stateVariable sendEvents="no">
      <name>LinkStatus</name>
      <dataType>string</dataType>
	  <defaultValue>"-"</defaultValue>
    </stateVariable>
	<stateVariable sendEvents="yes">
      <name>CurrentActivityID</name>
      <dataType>i4</dataType>
    </stateVariable>
	<stateVariable sendEvents="yes">
      <name>StartingActivityID</name>
      <dataType>i4</dataType>
    </stateVariable>
	<stateVariable sendEvents="yes">
      <name>StartingActivityStep</name>
      <dataType>string</dataType>
    </stateVariable>
	<stateVariable sendEvents="no">
      <name>LastCommand</name>
      <dataType>string</dataType>
    </stateVariable>
	<stateVariable sendEvents="no">
      <name>LastCommandTime</name>
      <dataType>string</dataType>
    </stateVariable>
	<stateVariable sendEvents="yes">
      <name>activityStatus</name>
      <dataType>ui1</dataType>
    </stateVariable>
	<stateVariable sendEvents="no">
      <name>RemoteImages</name>
      <dataType>ui1</dataType>
    </stateVariable>
	<stateVariable sendEvents="no">
      <name>Channel</name>
      <dataType>ui4</dataType>
    </stateVariable>
	<stateVariable sendEvents="no">
      <name>Polling</name>
      <dataType>ui1</dataType>
    </stateVariable>
	<stateVariable sendEvents="no">
      <name>LogLevel</name>
      <dataType>ui1</dataType>
    </stateVariable>
	<stateVariable sendEvents="no">
      <name>HubIPAddress</name>
      <dataType>string</dataType>
    </stateVariable>
	<stateVariable sendEvents="no">
      <name>DeviceID</name>
      <dataType>string</dataType>
    </stateVariable>
	<stateVariable sendEvents="no">
      <name>DeviceName</name>
      <dataType>string</dataType>
    </stateVariable>
	<stateVariable sendEvents="no">
      <name>Duration</name>
      <dataType>ui4</dataType>
	  <defaultValue>0</defaultValue>
    </stateVariable>
	<stateVariable sendEvents="no">
      <name>ActivityName</name>
      <dataType>string</dataType>
    </stateVariable>
	<stateVariable sendEvents="no">
      <name>ActivityID</name>
      <dataType>string</dataType>
    </stateVariable>
	<stateVariable sendEvents="no">
      <name>findResult</name>
      <dataType>string</dataType>
    </stateVariable>
  </serviceStateTable>
  <actionList>
    <action>
		<name>PowerOff</name>
    </action>
    <action>
		<name>StartActivity</name>
		<argumentList>
			<argument>
				<name>newActivityID</name>
				<direction>in</direction>
				<relatedStateVariable>CurrentActivityID</relatedStateVariable>
			</argument>
		</argumentList>
    </action>
    <action>
		<name>ChangeChannel</name>
		<argumentList>
			<argument>
				<name>newChannel</name>
				<direction>in</direction>
				<relatedStateVariable>Channel</relatedStateVariable>
			</argument>
		</argumentList>
    </action>
    <action>
		<name>GetCurrentActivityID</name>
		<argumentList>
			<argument>
				<name>currentActivityID</name>
				<direction>out</direction>
				<relatedStateVariable>CurrentActivityID</relatedStateVariable>
				<retval/>
			</argument>
		</argumentList>
    </action>
    <action>
		<name>IssueDeviceCommand</name>
		<argumentList>
			<argument>
				<name>DeviceID</name>
				<direction>in</direction>
				<relatedStateVariable>DeviceID</relatedStateVariable>
			</argument>
			<argument>
				<name>Command</name>
				<direction>in</direction>
				<relatedStateVariable>LastCommand</relatedStateVariable>
			</argument>
			<argument>
				<name>Duration</name>
				<direction>in</direction>
				<relatedStateVariable>Duration</relatedStateVariable>
			</argument>
		</argumentList>
	</action>
    <action>
		<name>SetHubPolling</name>
		<argumentList>
			<argument>
				<name>newPollFlag</name>
				<direction>in</direction>
				<relatedStateVariable>Polling</relatedStateVariable>
			</argument>
		</argumentList>
    </action>
    <action>
		<name>SetLogLevel</name>
		<argumentList>
			<argument>
				<name>newLogLevel</name>
				<direction>in</direction>
				<relatedStateVariable>LogLevel</relatedStateVariable>
			</argument>
		</argumentList>
    </action>
    <action>
		<name>SetRemoteImages</name>
		<argumentList>
			<argument>
				<name>newImageRemote</name>
				<direction>in</direction>
				<relatedStateVariable>RemoteImages</relatedStateVariable>
			</argument>
		</argumentList>
    </action>
    <action>
		<name>SetHubIPAddress</name>
		<argumentList>
			<argument>
				<name>newIPAddress</name>
				<direction>in</direction>
				<name>newPort</name>
				<direction>in</direction>
				<relatedStateVariable>HubIPAddress</relatedStateVariable>
			</argument>
		</argumentList>
    </action>
    <action>
		<name>FindActivityByName</name>
		<argumentList>
			<argument>
				<name>ActivityName</name>
				<direction>in</direction>
				<relatedStateVariable>ActivityName</relatedStateVariable>
			</argument>
			<argument>
				<name>retActivityID</name>
				<direction>out</direction>
				<relatedStateVariable>findResult</relatedStateVariable>
			</argument>
		</argumentList>
    </action>
    <action>
		<name>FindActivityByID</name>
		<argumentList>
			<argument>
				<name>ActivityID</name>
				<direction>in</direction>
				<relatedStateVariable>ActivityID</relatedStateVariable>
			</argument>
			<argument>
				<name>retActivityName</name>
				<direction>out</direction>
				<relatedStateVariable>findResult</relatedStateVariable>
			</argument>
		</argumentList>
    </action>
    <action>
		<name>FindDeviceByName</name>
		<argumentList>
			<argument>
				<name>DeviceName</name>
				<direction>in</direction>
				<relatedStateVariable>DeviceName</relatedStateVariable>
			</argument>
			<argument>
				<name>retDeviceID</name>
				<direction>out</direction>
				<relatedStateVariable>findResult</relatedStateVariable>
			</argument>
		</argumentList>
    </action>
    <action>
		<name>FindDeviceByID</name>
		<argumentList>
			<argument>
				<name>DeviceID</name>
				<direction>in</direction>
				<relatedStateVariable>DeviceID</relatedStateVariable>
			</argument>
			<argument>
				<name>retDeviceName</name>
				<direction>out</direction>
				<relatedStateVariable>findResult</relatedStateVariable>
			</argument>
		</argumentList>
    </action>
    <action>
		<name>UpdateButtons</name>
    </action>
    <action>
		<name>ForceUpdateConfiguration</name>
    </action>
  </actionList>
</scpd>
